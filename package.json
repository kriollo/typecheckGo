{
  "name": "go-typechecker",
  "version": "1.0.0",
  "description": "A type checker for Go written in TypeScript, providing static analysis for Go code.",
  "main": "dist/index.js",
  "bin": {
    "go-typecheck": "./dist/cli.js"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js",
    "test": "jest",
    "dev": "ts-node src/cli.ts",
    "clean": "rm -rf dist",
    "go:build:win": "go build -o tscheck.exe",
    "go:build:unix": "go build -o tscheck",
    "go:build:all": "npm run go:build:win && npm run go:build:unix",
    "release": "npm run build && npm run go:build:all",
    "version:patch": "npm version patch && npm run release",
    "version:minor": "npm version minor && npm run release",
    "version:major": "npm version major && npm run release",
    "publish:all": "npm run release && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kriollo/go-typechecker.git"
  },
  "keywords": [
    "go",
    "golang",
    "typechecker",
    "static-analysis",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "files": [
    "dist",
    "tscheck.exe",
    "tscheck"
  ],
  "bugs": {
    "url": "https://github.com/kriollo/go-typechecker/issues"
  },
  "homepage": "https://github.com/kriollo/go-typechecker#readme",
  "devDependencies": {
    "@types/execa": "^0.9.0",
    "@types/jest": "^29.5.0",
    "@types/jquery": "^3.5.33",
    "@types/node": "^24.10.1",
    "@types/vue": "^1.0.31",
    "go-typechecker": "file:go-typechecker-1.0.0.tgz",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "execa": "^9.6.0"
  }
}
